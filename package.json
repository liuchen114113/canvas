{
  "name": "canvas",
  "version": "0.2.0",
  "description": "canvas",
  "author": "Horizon Robotics",
  "template": "hobot-react-antd-ts-template",
  "scripts": {
    "start": "npm run dev",
    "build": "npm run prod",
    "dev": "webpack-dev-server --config webpack/webpack.dev.config.js --mode development --color --progress --hot --open",
    "prod": "webpack --config webpack/webpack.prod.config.js --mode production",
    "analyse": "npm run build --analyse",
    "test": "jest --no-cache",
    "lint": "tsc && npm run lint:ts && npm run lint:style",
    "lint:fix": "npm run lint:ts:fix && npm run lint:style:fix",
    "lint:ts": "tslint src/**/*.{ts,tsx}",
    "lint:ts:fix": "tslint src/**/*.{ts,tsx} --fix",
    "lint:style": "stylelint 'src/**/*.{css,less}'",
    "lint:style:fix": "stylelint 'src/**/*.{css,less}' --fix"
  },
  "dependencies": {
    "@horizon/dawn-ui": "^1.11.1",
    "@hot-loader/react-dom": "^16.8.6",
    "@loadable/component": "^5.10.2",
    "@rematch/core": "^1.1.0",
    "@rematch/loading": "^1.1.3",
    "antd": "^3.21.3",
    "axios": "^0.19.0",
    "bizcharts": "^3.5.5",
    "js-cookie": "^2.2.1",
    "moment": "^2.24.0",
    "path-to-regexp": "^3.0.0",
